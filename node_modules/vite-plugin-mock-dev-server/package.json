{
  "name": "vite-plugin-mock-dev-server",
  "type": "module",
  "version": "1.5.0",
  "packageManager": "pnpm@8.15.5",
  "author": "pengzhanbo <q942450674@outlook.com> (https://github.com/pengzhanbo)",
  "license": "MIT",
  "homepage": "https://vite-plugin-mock-dev-server.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pengzhanbo/vite-plugin-mock-dev-server"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "mock",
    "mock-server",
    "development"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18 || >= 20"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "dependencies": {
    "@pengzhanbo/utils": "^1.1.2",
    "@rollup/pluginutils": "^5.1.0",
    "chokidar": "^3.6.0",
    "co-body": "^6.1.0",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "esbuild": "^0.20.2",
    "fast-glob": "^3.3.2",
    "formidable": "2.1.2",
    "http-status": "^1.7.4",
    "is-core-module": "^2.13.1",
    "json5": "^2.2.3",
    "mime-types": "^2.1.35",
    "path-to-regexp": "^6.2.1",
    "picocolors": "^1.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^1.7.0",
    "@types/co-body": "^6.1.3",
    "@types/cookies": "^0.9.0",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/formidable": "2.0.6",
    "@types/is-core-module": "^2.2.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.10",
    "bumpp": "^9.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.57.0",
    "mockjs": "^1.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vitepress": "^1.0.1",
    "vitest": "^1.4.0"
  },
  "scripts": {
    "build": "tsup && node ./deprecate.mjs",
    "dev": "DEBUG=vite:mock-dev-server vite example --config ./example/vite.config.ts",
    "example:build": "DEBUG=vite:mock-dev-server vite build example --config ./example/vite.config.ts",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "test": "vitest",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --execute=\"pnpm release:changelog\" --commit --all --push --tag && pnpm publish --access public"
  }
}